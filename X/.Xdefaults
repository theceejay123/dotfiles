URxvt.depth: 32
URxvt*font:	xft: UbuntuMono Nerd Font Mono:pixelsize=14:minspace=False,
            xft: Monospace:pixelsize=20:minspace=False
                    
URxvt.scrollBar:	false

!! Extentions
URxvt.perl-ext-common:  default,matcher,resize-font,url-select,keyboard-select,clipboard

!!Resize-font
URxvt.resize-font.smaller:  C-j
URxvt.resize-font.bigger: C-k


!!St settings
*.font: UbuntuMono Nerd Font Mono:pixelsize=15:antialias=true:autohint:true;
*.alpha: 120


!!Clipboard
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M.v: perl:clipboard:paste

!! special
*foreground:   #bfcada
*background:   #0c1014
*cursorColor:  #bfcada

!! black
*color0:       #151a20
*color8:       #76ddd4

!! red
*color1:       #1d232a
*color9:       #f1b0f8

!! green
*color2:       #2c343e
*color10:      #c6a453

!! yellow
*color3:       #414c5a
*color11:      #8aee71

!! blue
*color4:       #5a697c
*color12:      #eba1f4

!! magenta
*color5:       #798da5
*color13:      #6db5ae

!! cyan
*color6:       #a4b6cc
*color14:      #6bd94a

!! white
*color7:       #dde3eb
*color15:      #68dfd5
